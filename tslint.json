{
  "extends": "tslint:recommended",
  "rules": {
    "no-reference": false,
    "space-before-function-paren": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock",
      "check-postbrace"
    ]
  }
}
